{
    "parser": {
        "name": "io.react2.scalata.parsers.MongoParser"
    },
    "exporter": {
        "name": "io.react2.scalata.exporters.ConsoleExporter"
    },
    "data_structure": {
        "repeat": 100,
        "fields": [
            {
                "type": "<_id>",
                "name": "_id"
            },
            {
                "type": "{{int-64}}",
                "name": "timestamp"
            },
            {
                "type": "{{isodate}}",
                "name": "createdAt"
            },
            {
                "type": "{{object}}",
                "name": "club",
                "fields": [
                    {
                        "type": "{{string}}",
                        "name": "name"
                    },
                    {
                        "type": "{{int-32}}",
                        "name": "age"
                    }
                ]
            }
        ]
    }
}
