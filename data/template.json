{
    "parser": "MongoParser",
    "exporter": {
        "name": "ConsoleExporter"
    },
    "data_structure": {
        "repeat": 1000000,
        "fields": [
            {
                "type": "{{int-64}}",
                "name": "timestamp"
            },
            {
                "type": "{{date}}",
                "name": "birthday"
            },
            {
                "type": "{{object}}",
                "name": "club",
                "fields": [
                    {
                        "type": "{{string}}",
                        "name": "name"
                    },
                    {
                        "type": "{{int-32}}",
                        "name": "age"
                    }
                ]
            }
        ]
    }
}
